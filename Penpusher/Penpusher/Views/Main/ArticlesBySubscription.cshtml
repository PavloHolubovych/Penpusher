<div id="articlesSubscriptions">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Articles by subscription "<span data-bind="text: provider"></span>"</h1>
        </div>
    </div>
    <div class="loadimage">
        <img src="~/fonts/load.gif" />
    </div>
    <div class="articles" data-bind="foreach: rows">
        <div class="row" data-bind="foreach: $data">
            @Html.Partial("ArticlesPartialView")
        </div>
    </div>
</div>
<script src="~/Scripts/GetAllArticlesByProvider.js"></script>